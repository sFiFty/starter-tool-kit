{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel",
  ],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "components": "./app/components",
        "containers": "./app/containers",
        "screens": "./app/screens",
        "utils": "./app/utils",
        "types": "./app/types",
        "services": "./app/services",
        "styles": "./app/styles",
        "static": "./static",
      }
    }]
  ]
}